{
  "name": "chatcord",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "GPL-3.0",
  "main": "dist/main.js",
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node .",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "type-check": "tsc --noEmit --noErrorTruncation --incremental false",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.0.5",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/websockets": "^7.0.5",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.10",
    "@types/jest": "25.1.4",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.7.7",
    "@types/node": "^13.9.1",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-no-secrets": "^0.6.5",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-wes": "^3.0.2",
    "jest": "^25.1.0",
    "lint-staged": "^10.1.1",
    "mockingoose": "^2.13.2",
    "patch-package": "^6.2.1",
    "prettier": "^2.0.2",
    "prettier-plugin-import-sort": "0.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "projects": [
      {
        "displayName": "Unit",
        "rootDir": "src",
        "moduleNameMapper": {
          "^~app/(.*)$": "<rootDir>/$1",
          "^~chat/(.*)$": "<rootDir>/chat/$1",
          "^~auth/(.*)$": "<rootDir>/auth/$1"
        },
        "testEnvironment": "node",
        "preset": "ts-jest"
      },
      {
        "displayName": "E2E",
        "rootDir": "test",
        "moduleNameMapper": {
          "^~app/(.*)$": "<rootDir>/../src/$1",
          "^~chat/(.*)$": "<rootDir>/../src/chat/$1",
          "^~auth/(.*)$": "<rootDir>/../src/auth/$1"
        },
        "testEnvironment": "node",
        "preset": "@shelf/jest-mongodb",
        "testRegex": ".e2e-spec.ts$",
        "transform": {
          "^.+\\.ts$": "ts-jest"
        }
      }
    ]
  }
}
